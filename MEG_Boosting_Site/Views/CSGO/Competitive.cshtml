@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<!---<link rel="stylesheet" href="~/css/csgo.css" /> -->

<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

<div class="body_container">
	<section id="new-header-section" class="">
		<div class="bg_container">
			<div id="back_id" class="main_bg_image"></div>
			<div class="container">
				<div class="sml_bg">
					<img src="images/sml_bg_csgo.png" alt="">
				</div>
				<div class="fog_bg"></div>
			</div>
		</div>
		<div class="title_container">
			<div class="fixed_container"></div>
			<div class="fill_container">
				<div class="container">
					<h1 id="title_id">Classic CS:GO Ranks</h1>
					<div class="green_divider"></div>
					<p class="subtitle">Big selection of ranks to choose from!</p>
				</div>
			</div>
	        
			<div class="row productTypeContent active maximizeBelow1200" data-id="0" data-type-name="Classic Boosting" data-type="diff" style="display: block; width: 70%; border: 1px solid #9e9e9e; padding: 25px; margin-top :20px; margin-bottom: 100px; margin-left: 0px; margin-right: 0px;"><div class="col-md-6" style="float: left;">
					<p style="float: left; width:100%;">
						<span style="color: #787878; font-size: 18px; font-weight: 500;">Steam Username</span>
						<span>
							<input type="text" class="form-control gg-form productOption" data-name="Steam Username" name="option-steam username">
						</span>
						<span class="productOptionError"></span>
					</p>
					<p style="float: left; width:100%; display: block;" id="0c12cc567942a86442c15b03d954dad2">
						<span style="color: #787878; font-size: 18px; font-weight: 500;">Queue Type</span>
						<span>
							<select class="form-control gg-form custom-select productOption" data-name="Queue Type" name="option-Queue Type"><option data-products="" value="Solo Boost" data-price="percentage" data-pricevalue="0.0" data-disabled="false">Solo Boost</option><option data-products="" value="Duo Boost" data-price="percentage" data-pricevalue="60" data-disabled="false">Duo Boost [+60%]</option></select>
						</span>
						<span class="productOptionError">
						</span>
					</p>
				</div>
				<div class="col-md-6" style="float: left;">
					<p style="float: left; width:100%;">
						<span style="color: #787878; font-size: 18px; font-weight: 500;">Current Rank</span>
						<span id="Playerrank">
							<select class="ranks" name="ranks['currentrank']" id="crank" onchange="calculateAmount(this.value)" required>
								<option value="" disabled selected>Choose your current rank</option>
								<option value="1">Silver I</option>
								<option value="2">Silver II</option>
								<option value="3">Silver III</option>
								<option value="4">Silver IV</option>
								<option value="5">Silver Elite</option>
								<option value="6">Silver Elite Master</option>
								<option value="7">Gold Nova I</option>
								<option value="8">Gold Nova II</option>
								<option value="9">Gold Nova III</option>
								<option value="10">Gold Nova Master</option>
								<option value="11">Master Guardian I</option>
								<option value="12">Master Guardian II</option>
								<option value="13">Master Guardian Elite</option>
								<option value="14">Distinguished Master Guardian</option>
								<option value="15">Legendary Eagle</option>
								<option value="16">Legendary Eagle Master</option>
								<option value="17">Supreme Master First Class</option>
								<option value="18">Global Elite</option>
							</select>
						</span>
					</p>
					<p style="float: left; width:100%;">
						<span style="color: #787878; font-size: 18px; font-weight: 500;">Desired Rank</span>
						<span id="BoostRank">
							<select class="ranks" name="ranks['desiredrank']" id="drank" required>
								<option value="" disabled selected>Choose your desired rank</option>
								<option value="1">Silver I</option>
								<option value="2">Silver II</option>
								<option value="3">Silver III</option>
								<option value="4">Silver IV</option>
								<option value="5">Silver Elite</option>
								<option value="6">Silver Elite Master</option>
								<option value="7">Gold Nova I</option>
								<option value="8">Gold Nova II</option>
								<option value="9">Gold Nova III</option>
								<option value="10">Gold Nova Master</option>
								<option value="11">Master Guardian I</option>
								<option value="12">Master Guardian II</option>
								<option value="13">Master Guardian Elite</option>
								<option value="14">Distinguished Master Guardian</option>
								<option value="15">Legendary Eagle</option>
								<option value="16">Legendary Eagle Master</option>
								<option value="17">Supreme Master First Class</option>
								<option value="18">Global Elite</option>
							</select>
						</span>
						<br>
					</p>
					<p>
						<strong>Amount (US$)</strong>: <input type="text" name="amount" id="amount" />
					</p>
					<div>
						<input type="text" name="tot_amount" id="tot_amount" readonly>
					</div>
					<p style="float: left; width:100%;">
						<span style="color: #787878; font-size: 18px; font-weight: 500;">Add to cart.</span>
						<span>
							<input style="color: #787878" type="submit" value="Add to cart." placeholder="Add to cart." class="form-control btn-dark" data-name="Steam Username" name="option-steam username" v-on:click="clicked" onclick="">
						</span>
						<span class="productOptionError"></span>
					</p>
				</div><br><br></div>
		</div>
	</section>
</div>

