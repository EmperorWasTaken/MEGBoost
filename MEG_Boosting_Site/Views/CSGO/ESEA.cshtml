@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<link rel="stylesheet" href="~/css/csgo.css" />

<div class="body_container">
    <section id="new-header-section" class="">
        <div class="bg_container">
            <div id="back_id" class="main_bg_image"></div>
            <div class="container">
                <div class="sml_bg">
                    <img src="images/sml_bg_csgo.png" alt="">
                </div>
                <div class="fog_bg"></div>
            </div>
        </div>
        <div class="title_container">
            <div class="fixed_container"></div>
	        <div class="fill_container">
		        <div class="container">
			        <h1 id="title_id">ESEA Boosting</h1>
			        <div class="green_divider"></div>
			        <p class="subtitle">Big selection of ranks to choose from!</p>
		        </div>
	        </div>
	        
	        <div class="row productTypeContent active maximizeBelow1200" data-id="0" data-type-name="Classic Boosting" data-type="diff" style="display: block; width: 70%; border: 1px solid #9e9e9e; padding: 25px; margin-top :20px; margin-bottom: 100px; margin-left: 0px; margin-right: 0px;"><div class="col-md-6" style="float: left;">
		        <p style="float: left; width:100%;">
			        <span style="color: #787878; font-size: 18px; font-weight: 500;">ESEA Username</span>
			        <span>
				        <input type="text" class="form-control gg-form productOption" data-name="Steam Username" name="option-steam username">
			        </span>
			        <span class="productOptionError"></span>
		        </p>
		        <p style="float: left; width:100%; display: block;" id="0c12cc567942a86442c15b03d954dad2">
			        <span style="color: #787878; font-size: 18px; font-weight: 500;">Queue Type</span>
			        <span>
				        <select class="form-control gg-form custom-select productOption" data-name="Queue Type" name="option-Queue Type">
					        <option data-products="" value="Solo Boost" data-price="percentage" data-pricevalue="0.0" data-disabled="false">Solo Boost</option>
					        <option data-products="" value="Duo Boost" data-price="percentage" data-pricevalue="60" data-disabled="false">Duo Boost [+60%]</option>
				        </select>
			        </span>
			        <span class="productOptionError">
			        </span>
		        </p>
	        </div>
		        <div class="col-md-6" style="float: left;">
		        <p style="float: left; width:100%;">
			        <span style="color: #787878; font-size: 18px; font-weight: 500;">Current Rank</span>
			        <span>
				        <select class="form-control gg-form custom-select product-start handle-price-change" name="currentDivision" data-name="Current Rank">
					        <option value="293">D-</option>
					        <option value="294">D</option>
					        <option value="295">D+</option>
					        <option value="296">C-</option>
					        <option value="297">C</option>
					        <option value="298">C+</option>
					        <option value="299">B-</option>
					        <option value="300">B</option>
					        <option value="301">B+</option>
					        <option value="302">A-</option>
					        <option value="303">A</option>
					        <option value="304">A+</option>
					        <option value="305">G</option>
				        </select>
			        </span>
		        </p>
		        <p style="float: left; width:100%;">
			        <span style="color: #787878; font-size: 18px; font-weight: 500;">Desired Rank</span>
			        <span>
				        <select class="form-control gg-form custom-select product-end handle-price-change" data-name="Desired Rank" name="desiredDivision">
					        <option value="293">D-</option>
					        <option value="294">D</option>
					        <option value="295">D+</option>
					        <option value="296">C-</option>
					        <option value="297">C</option>
					        <option value="298">C+</option>
					        <option value="299">B-</option>
					        <option value="300">B</option>
					        <option value="301">B+</option>
					        <option value="302">A-</option>
					        <option value="303">A</option>
					        <option value="304">A+</option>
					        <option value="305">G</option>
				        </select>
			        </span>
			        <br>
		        </p>
		        <p style="float: left; width:100%;">
			        <span style="color: #787878; font-size: 18px; font-weight: 500;">Add to cart.</span>
			        <span>
				        <input style="color: #787878" type="submit" value="Add to cart." placeholder="Add to cart." class="form-control btn-dark" onclick="">
			        </span>
			        <span class="productOptionError"></span>
		        </p>
	        </div><br><br></div>
	        
        </div>
    </section>
</div>