@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<link rel="stylesheet" href="~/css/csgo.css" />

<div class="body_container">
    <section id="new-header-section" class="">
        <div class="bg_container">
            <div id="back_id" class="main_bg_image"></div>
            <div class="container">
                <div class="sml_bg">
                    <img src="images/sml_bg_csgo.png" alt="" width="100%">
                </div>
                <div class="fog_bg"></div>
            </div>
        </div>
        <div class="title_container">
            <div class="fixed_container"></div>
            <div class="fill_container">
                <div class="container">
                    <h1 id="title_id">CSGO Boost to Desired Rank</h1>
                    <div class="green_divider"></div>
                    <p class="subtitle">Big Sale on CS:GO Boosting!</p>
                </div>
            </div>
            <div class="fixed_container">
                <div class="buttons_links">
                    <a href="https://localhost:5001/CSGO/Competitive">
                        <div class="button_link">
                            <span>Classic CS:GO Ranks</span>
                        </div>
                    </a>
                    <a href="https://localhost:5001/CSGO/ESEA">
                        <div class="button_link">
                            <span>ESEA Boosting</span>
                        </div>
                    </a>
                    <div class="button_link">
                         <span>
                             <a href="https://localhost:5001/CSGO/Faceit">
                                 <span>Faceit Boosting</span>
                             </a>
                        </span>
                     </div>
                    <a asp-route="competitive">
                        <div class="button_link">
                            <span>Wingman Ranks</span>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    </section>
</div>