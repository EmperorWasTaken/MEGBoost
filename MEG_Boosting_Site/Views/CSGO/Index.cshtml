    <div class="container">
        <h1 >CSGO Boost to Desired Rank</h1>
        <p class="subtitle">Big Sale on CS:GO Boosting!</p>
    </div>
</div>
<div class="flex-container">
    <input type="button" value="Classic CS:GO Ranks" onclick="location.href='@Url.Action("Competitive", "CSGO")'"/>
    <input type="button" value="ESEA Boosting" onclick="location.href='@Url.Action("ESEA", "CSGO")'"/>
    <input type="button" value="Faceit Boosting" onclick="location.href='@Url.Action("Faceit", "CSGO")'"/>
    <input type="button" value="Wingman Ranks" onclick="location.href='@Url.Action("Wingman", "CSGO")'"/>
</div>